  },
  "files": [
    "/patches",
    "/scripts"
  ],
  "homepage": "https://rebrowser.net",
  "main": "scripts/enhanced-patcher.js",
  "bin": {
    "rebrowser-stealth": "./scripts/enhanced-patcher.js",
    "rebrowser-patches": "./scripts/enhanced-patcher.js",
    "rebrowser-enhanced": "./scripts/enhanced-patcher.js"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node ./scripts/stealth-patcher.js",
    "patch": "node ./scripts/enhanced-patcher.js patch",
    "stealth-patch": "node ./scripts/stealth-patcher.js stealth-patch",
    "stealth-patch-advanced": "node ./scripts/stealth-patcher.js stealth-patch --stealthMode=comprehensive",
    "stealth-test": "node ./scripts/stealth-patcher.js test-stealth",
    "patch-puppeteer": "node ./scripts/enhanced-patcher.js patch-comprehensive --packageName=puppeteer-core",
    "patch-playwright": "node ./scripts/enhanced-patcher.js patch-comprehensive --packageName=playwright-core",
    "patch-puppeteer-basic": "node ./scripts/patcher.js patch --packageName=puppeteer-core --codeTarget=lib && node ./scripts/patcher.js patch --packageName=puppeteer-core --codeTarget=src",
    "patch-playwright-basic": "node ./scripts/patcher.js patch --packageName=playwright-core --codeTarget=lib && node ./scripts/patcher.js patch --packageName=playwright-core --codeTarget=src",
    "enhanced-patch": "node ./scripts/enhanced-patcher.js patch-comprehensive",
    "patch-both": "npm run patch-puppeteer && npm run patch-playwright",
    "setup-puppeteer": "npm install puppeteer-core && npm run patch-puppeteer && echo ðŸŽ­ Puppeteer-core setup completed with comprehensive stealth patches!",
    "setup-playwright": "npm install playwright-core && npm run patch-playwright && echo ðŸŽª Playwright-core setup completed with comprehensive stealth patches!",
    "setup-both": "npm install puppeteer-core playwright-core && npm run patch-both && echo ðŸŽ¯ Both engines setup completed with comprehensive stealth!",
    "install-puppeteer": "npm install puppeteer-core && npm run patch-puppeteer",
    "install-playwright": "npm install playwright-core && npm run patch-playwright",
    "test-puppeteer": "node ./test.js --engine=puppeteer --keepOpen",
    "test-playwright": "node ./test.js --engine=playwright --keepOpen",
    "test-puppeteer-headless": "node ./test.js --engine=puppeteer --headless=true --keepOpen",
    "test-playwright-headless": "node ./test.js --engine=playwright --headless=true --keepOpen",
    "test-auto": "node ./test.js --engine=puppeteer --keepOpen",
    "test-stealth": "node ./test.js --engine=playwright --keepOpen",
    "test-detection": "npm run test-comprehensive",
    "test-comprehensive": "echo 'Testing Puppeteer...' && npm run test-puppeteer && echo 'Testing Playwright...' && npm run test-playwright",
    "test-both": "npm run test-comprehensive",
    "ai-test": "node ./scripts/ai-agent.js",
    "ai-agent": "node ./scripts/ai-agent.js",
    "test-ai": "node ./scripts/ai-agent.js",
    "create-brave-puppeteer": "node ./scripts/create-brave-package.js --engine=puppeteer",
    "create-brave-playwright": "node ./scripts/create-brave-package.js --engine=playwright",
    "create-brave-packages": "npm run create-brave-puppeteer && npm run create-brave-playwright",
    "publish-brave-packages": "node ./scripts/publish-brave-packages.js",
    "publish-brave-puppeteer": "node ./scripts/publish-brave-packages.js --package=puppeteer",
    "publish-brave-playwright": "node ./scripts/publish-brave-packages.js --package=playwright",
    "publish-brave-dry-run": "node ./scripts/publish-brave-packages.js --dry-run",
    "build-and-publish-brave": "npm run create-brave-packages && npm run publish-brave-packages",
    "clean": "node ./scripts/clean-project.js",
    "validate": "node ./scripts/validate-patches.js",
    "setup": "node ./scripts/setup-wizard.js",
    "check-versions": "node ./scripts/check-versions.js",
    "check-versions-only": "node ./scripts/check-versions.js --check-only",
    "test": "node ./test.js --engine=puppeteer --keepOpen"
  },
  "dependencies": {
    "yargs": "^17.7.2"
  },
  "optionalDependencies": {
    "playwright-core": "^1.55.0",
    "puppeteer-core": "^24.20.0"
  },
  "devDependencies": {
    "test": "^3.3.0"
  }
}
